    {
      "cell_type": "code",
      "source": [
        "print(\"=\" * 60)\n",
        "print(\"STEP 2: TOKENIZATION\")\n",
        "print(\"=\" * 60)\n",
        "\n",
        "print(\"🔤 Loading tokenizer (GPT-2 BPE)...\")\n",
        "tokenizer = tiktoken.get_encoding(\"gpt2\")\n",
        "vocab_size = tokenizer.n_vocab  # 50,257 tokens\n",
        "\n",
        "print(f\"✅ Vocabulary size: {vocab_size:,} tokens\\n\")\n",
        "\n",
        "# Demo: See tokenization in action\n",
        "example_text = \"Hello, world! This is a test.\"\n",
        "tokens = tokenizer.encode(example_text)\n",
        "\n",
        "print(\"💡 Tokenization Demo:\")\n",
        "print(f\"   Text:   '{example_text}'\")\n",
        "print(f\"   Tokens: {tokens}\")\n",
        "print(f\"   Count:  {len(tokens)} tokens\\n\")\n",
        "\n",
        "# Decode back\n",
        "decoded = tokenizer.decode(tokens)\n",
        "print(f\"   Decoded: '{decoded}'\\n\")\n",
        "\n",
        "# Tokenize entire dataset\n",
        "print(\"🔄 Tokenizing all stories...\")\n",
        "def tokenize_story(example):\n",
        "    \"\"\"Tokenize a single story.\"\"\"\n",
        "    tokens = tokenizer.encode(example['text'])\n",
        "    return {'tokens': tokens}\n",
        "\n",
        "tokenized_dataset = dataset.map(\n",
        "    tokenize_story,\n",
        "    remove_columns=['text'],\n",
        "    desc=\"Tokenizing\"\n",
        ")\n",
        "\n",
        "print(f\"✅ Tokenization complete!\")\n",
        "print()"
      ],
