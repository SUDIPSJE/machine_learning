    {
      "cell_type": "code",
      "source": [
        "print(\"=\" * 60)\n",
        "print(\"STEP 4: BUILD MODEL ARCHITECTURE\")\n",
        "print(\"=\" * 60)\n",
        "\n",
        "class GemmaConfig:\n",
        "    \"\"\"Configuration for Gemma 3 270M model (simplified).\"\"\"\n",
        "\n",
        "    vocab_size: int = 50257       # Tokenizer vocabulary\n",
        "    hidden_size: int = 768        # Embedding dimension\n",
        "    num_layers: int = 16          # Transformer layers\n",
        "    num_heads: int = 8            # Attention heads per layer\n",
        "    max_seq_length: int = 128     # Context window\n",
        "    dropout: float = 0.1          # Regularization\n",
        "\n",
        "    @property\n",
        "    def num_parameters(self):\n",
        "        \"\"\"Approximate parameter count.\"\"\"\n",
        "        # This is a rough estimate\n",
        "        emb = self.vocab_size * self.hidden_size\n",
        "        attn = self.num_layers * (4 * self.hidden_size ** 2)\n",
        "        ffn = self.num_layers * (2 * 4 * self.hidden_size ** 2)\n",
        "        return emb + attn + ffn\n",
        "\n",
        "\n",
        "config = GemmaConfig()\n",
        "\n",
        "print(f\"📝 Model Configuration:\")\n",
        "print(f\"   Vocabulary: {config.vocab_size:,} tokens\")\n",
        "print(f\"   Hidden size: {config.hidden_size}\")\n",
        "print(f\"   Layers: {config.num_layers}\")\n",
        "print(f\"   Attention heads: {config.num_heads}\")\n",
        "print(f\"   Max sequence: {config.max_seq_length}\")\n",
        "print(f\"   Parameters: ~{config.num_parameters / 1e6:.1f}M\")\n",
        "print()\n",
        "\n",
        "\n",
        "class TransformerBlock(nn.Module):\n",
        "    \"\"\"\n",
        "    Single transformer block (self-attention + feedforward).\n",
        "    This is the building block that gets repeated 16 times.\n",
        "    \"\"\"\n",
        "\n",
        "    def __init__(self, config):\n",
        "        super().__init__()\n",
        "\n",
        "        # Multi-head self-attention\n",
        "        self.attention = nn.MultiheadAttention(\n",
        "            embed_dim=config.hidden_size,\n",
        "            num_heads=config.num_heads,\n",
        "            dropout=config.dropout,\n",
        "            batch_first=True\n",
        "        )\n",
        "\n",
        "        # Feed-forward network\n",
        "        self.ffn = nn.Sequential(\n",
        "            nn.Linear(config.hidden_size, 4 * config.hidden_size),\n",
        "            nn.GELU(),\n",
        "            nn.Linear(4 * config.hidden_size, config.hidden_size),\n",
        "            nn.Dropout(config.dropout)\n",
        "        )\n",
        "\n",
        "        # Layer normalization\n",
        "        self.ln1 = nn.LayerNorm(config.hidden_size)\n",
        "        self.ln2 = nn.LayerNorm(config.hidden_size)\n",
        "\n",
        "    def forward(self, x):\n",
        "        # Self-attention with residual connection\n",
        "        attn_out, _ = self.attention(x, x, x, need_weights=False)\n",
        "        x = x + attn_out\n",
        "        x = self.ln1(x)\n",
        "\n",
        "        # Feed-forward with residual connection\n",
        "        ffn_out = self.ffn(x)\n",
        "        x = x + ffn_out\n",
        "        x = self.ln2(x)\n",
        "\n",
        "        return x\n",
        "\n",
        "\n",
        "class GemmaModel(nn.Module):\n",
        "    \"\"\"\n",
        "    Complete Gemma 3 270M model architecture.\n",
        "\n",
        "    Flow:\n",
        "    Token IDs → Embeddings → 16 Transformer Blocks → Output Logits\n",
        "    \"\"\"\n",
        "\n",
        "    def __init__(self, config):\n",
        "        super().__init__()\n",
        "        self.config = config\n",
        "\n",
        "        # Token embeddings (convert token IDs to vectors)\n",
        "        self.token_embedding = nn.Embedding(config.vocab_size, config.hidden_size)\n",
        "\n",
        "        # Position embeddings (where is this token in the sequence?)\n",
        "        self.position_embedding = nn.Embedding(config.max_seq_length, config.hidden_size)\n",
        "\n",
        "        # Transformer blocks (16 layers of self-attention + FFN)\n",
        "        self.blocks = nn.ModuleList([\n",
        "            TransformerBlock(config) for _ in range(config.num_layers)\n",
        "        ])\n",
        "\n",
        "        # Final layer norm\n",
        "        self.ln_final = nn.LayerNorm(config.hidden_size)\n",
        "\n",
        "        # Output projection (hidden → vocabulary logits)\n",
        "        self.lm_head = nn.Linear(config.hidden_size, config.vocab_size, bias=False)\n",
        "\n",
        "        # Initialize weights\n",
        "        self.apply(self._init_weights)\n",
        "\n",
        "    def _init_weights(self, module):\n",
        "        \"\"\"Initialize weights with small random values.\"\"\"\n",
        "        if isinstance(module, nn.Linear):\n",
        "            torch.nn.init.normal_(module.weight, mean=0.0, std=0.02)\n",
        "            if module.bias is not None:\n",
        "                torch.nn.init.zeros_(module.bias)\n",
        "        elif isinstance(module, nn.Embedding):\n",
        "            torch.nn.init.normal_(module.weight, mean=0.0, std=0.02)\n",
        "\n",
        "    def forward(self, input_ids):\n",
        "        batch_size, seq_length = input_ids.shape\n",
        "\n",
        "        # Get embeddings\n",
        "        token_emb = self.token_embedding(input_ids)\n",
        "\n",
        "        # Add position information\n",
        "        positions = torch.arange(0, seq_length, device=input_ids.device)\n",
        "        position_emb = self.position_embedding(positions)\n",
        "\n",
        "        x = token_emb + position_emb.unsqueeze(0)\n",
        "\n",
        "        # Pass through transformer blocks\n",
        "        for block in self.blocks:\n",
        "            x = block(x)\n",
        "\n",
        "        # Final layer norm\n",
        "        x = self.ln_final(x)\n",
        "\n",
        "        # Project to vocabulary\n",
        "        logits = self.lm_head(x)\n",
        "\n",
        "        return logits\n",
        "\n",
        "    def count_parameters(self):\n",
        "        \"\"\"Count trainable parameters.\"\"\"\n",
        "        return sum(p.numel() for p in self.parameters() if p.requires_grad)\n",
        "\n",
        "\n",
        "# Create model\n",
        "print(\"🏗️  Building Gemma 3 270M model...\")\n",
        "model = GemmaModel(config).to(device)\n",
        "\n",
        "total_params = model.count_parameters()\n",
        "print(f\"✅ Model created!\")\n",
        "print(f\"   Total parameters: {total_params:,} ({total_params / 1e6:.1f}M)\")\n",
        "print(f\"   Model size: ~{total_params * 4 / 1e9:.2f} GB (float32)\")\n",
        "print()\n",
        "\n",
        "# Show model structure\n",
        "print(\"📐 Model Architecture:\")\n",
        "print(model)\n",
        "print()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 0
        },
        "id": "h5VIyYIIM1O2",
        "outputId": "18f06a34-1498-4492-b56b-3dcce61a5c31"
      },
      "execution_count": 12,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "============================================================\n",
            "STEP 4: BUILD MODEL ARCHITECTURE\n",
            "============================================================\n",
            "📝 Model Configuration:\n",
            "   Vocabulary: 50,257 tokens\n",
            "   Hidden size: 768\n",
            "   Layers: 16\n",
            "   Attention heads: 8\n",
            "   Max sequence: 128\n",
            "   Parameters: ~151.8M\n",
            "\n",
            "🏗️  Building Gemma 3 270M model...\n",
            "✅ Model created!\n",
            "   Total parameters: 190,700,544 (190.7M)\n",
            "   Model size: ~0.76 GB (float32)\n",
            "\n",
            "📐 Model Architecture:\n",
            "GemmaModel(\n",
            "  (token_embedding): Embedding(50257, 768)\n",
            "  (position_embedding): Embedding(128, 768)\n",
            "  (blocks): ModuleList(\n",
            "    (0-15): 16 x TransformerBlock(\n",
            "      (attention): MultiheadAttention(\n",
            "        (out_proj): NonDynamicallyQuantizableLinear(in_features=768, out_features=768, bias=True)\n",
            "      )\n",
            "      (ffn): Sequential(\n",
            "        (0): Linear(in_features=768, out_features=3072, bias=True)\n",
            "        (1): GELU(approximate='none')\n",
            "        (2): Linear(in_features=3072, out_features=768, bias=True)\n",
            "        (3): Dropout(p=0.1, inplace=False)\n",
            "      )\n",
            "      (ln1): LayerNorm((768,), eps=1e-05, elementwise_affine=True)\n",
            "      (ln2): LayerNorm((768,), eps=1e-05, elementwise_affine=True)\n",
            "    )\n",
            "  )\n",
            "  (ln_final): LayerNorm((768,), eps=1e-05, elementwise_affine=True)\n",
            "  (lm_head): Linear(in_features=768, out_features=50257, bias=False)\n",
            ")\n",
            "\n"
          ]
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "print(\"=\" * 60)\n",
        "print(\"STEP 5: TRAINING LOOP\")\n",
        "print(\"=\" * 60)\n",
        "\n",
        "# Training hyperparameters\n",
        "LEARNING_RATE = 3e-4\n",
        "NUM_EPOCHS = 10  # For demo; increase to 3-5 for better results\n",
        "GRAD_ACCUM_STEPS = 4  # Simulate larger batch size\n",
        "\n",
        "# Optimizer (adjusts model parameters to reduce loss)\n",
        "optimizer = torch.optim.AdamW(model.parameters(), lr=LEARNING_RATE)\n",
        "\n",
        "# Learning rate scheduler (gradually decrease learning rate)\n",
        "scheduler = torch.optim.lr_scheduler.CosineAnnealingLR(\n",
        "    optimizer,\n",
        "    T_max=len(train_loader) * NUM_EPOCHS\n",
        ")\n",
        "\n",
        "# Loss function (measures how wrong predictions are)\n",
        "loss_fn = nn.CrossEntropyLoss()\n",
        "\n",
        "print(f\"⚙️  Training Configuration:\")\n",
        "print(f\"   Learning rate: {LEARNING_RATE}\")\n",
        "print(f\"   Epochs: {NUM_EPOCHS}\")\n",
        "print(f\"   Gradient accumulation: {GRAD_ACCUM_STEPS} steps\")\n",
        "print(f\"   Effective batch size: {BATCH_SIZE * GRAD_ACCUM_STEPS}\")\n",
        "print()\n",
        "\n",
        "print(\"🔥 Starting training...\")\n",
        "print(\"=\" * 60)\n",
        "print()\n",
        "\n",
        "model.train()\n",
        "total_steps = 0\n",
        "losses = []\n",
        "\n",
        "for epoch in range(NUM_EPOCHS):\n",
        "    print(f\"📚 Epoch {epoch + 1}/{NUM_EPOCHS}\")\n",
        "    print(\"-\" * 60)\n",
        "\n",
        "    epoch_loss = 0\n",
        "    optimizer.zero_grad()\n",
        "\n",
        "    progress_bar = tqdm(train_loader, desc=f\"Training\")\n",
        "\n",
        "    for batch_idx, (inputs, targets) in enumerate(progress_bar):\n",
        "        # Move to device\n",
        "        inputs = inputs.to(device)\n",
        "        targets = targets.to(device)\n",
        "\n",
        "        # Forward pass\n",
        "        logits = model(inputs)\n",
        "\n",
        "        # Calculate loss\n",
        "        # Reshape for cross-entropy: (batch * seq_length, vocab_size)\n",
        "        loss = loss_fn(\n",
        "            logits.view(-1, config.vocab_size),\n",
        "            targets.view(-1)\n",
        "        )\n",
        "\n",
        "        # Normalize loss for gradient accumulation\n",
        "        loss = loss / GRAD_ACCUM_STEPS\n",
        "\n",
        "        # Backward pass\n",
        "        loss.backward()\n",
        "\n",
        "        # Update weights every GRAD_ACCUM_STEPS\n",
        "        if (batch_idx + 1) % GRAD_ACCUM_STEPS == 0:\n",
        "            # Clip gradients to prevent explosion\n",
        "            torch.nn.utils.clip_grad_norm_(model.parameters(), max_norm=1.0)\n",
        "\n",
        "            # Update parameters\n",
        "            optimizer.step()\n",
        "            scheduler.step()\n",
        "            optimizer.zero_grad()\n",
        "\n",
        "            total_steps += 1\n",
        "\n",
        "        # Track loss\n",
        "        epoch_loss += loss.item() * GRAD_ACCUM_STEPS\n",
        "        losses.append(loss.item() * GRAD_ACCUM_STEPS)\n",
        "\n",
        "        # Update progress bar\n",
        "        progress_bar.set_postfix({\n",
        "            'loss': f'{loss.item() * GRAD_ACCUM_STEPS:.4f}',\n",
        "            'lr': f'{scheduler.get_last_lr()[0]:.2e}'\n",
        "        })\n",
        "\n",
        "    avg_loss = epoch_loss / len(train_loader)\n",
        "    print(f\"📊 Epoch {epoch + 1} complete - Average loss: {avg_loss:.4f}\\n\")\n",
        "\n",
        "print(\"=\" * 60)\n",
        "print(\"✅ TRAINING COMPLETE!\")\n",
        "print(\"=\" * 60)\n",
        "print()\n",
        "\n",
        "# Save the model\n",
        "model_path = \"gemma_270m_trained.pt\"\n",
        "torch.save({\n",
        "    'model_state_dict': model.state_dict(),\n",
        "    'config': config,\n",
        "    'tokenizer': 'gpt2'\n",
        "}, model_path)\n",
        "\n",
        "print(f\"💾 Model saved to: {model_path}\")\n",
        "print()\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "mq653L13NNVn",
        "outputId": "33b8e2e0-7e03-41a9-82f7-c483031d5d20"
      },
      "execution_count": 16,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "============================================================\n",
            "STEP 5: TRAINING LOOP\n",
            "============================================================\n",
            "⚙️  Training Configuration:\n",
            "   Learning rate: 0.0003\n",
            "   Epochs: 4\n",
            "   Gradient accumulation: 4 steps\n",
            "   Effective batch size: 64\n",
            "\n",
            "🔥 Starting training...\n",
            "============================================================\n",
            "\n",
            "📚 Epoch 1/4\n",
            "------------------------------------------------------------\n"
          ]
        },
        {
          "output_type": "stream",
