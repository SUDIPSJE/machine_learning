    {
      "cell_type": "code",
      "source": [
        "print(\"=\" * 60)\n",
        "print(\"STEP 6: TESTING YOUR MODEL\")\n",
        "print(\"=\" * 60)\n",
        "\n",
        "def generate_text(model, tokenizer, prompt, max_length=100, temperature=0.8):\n",
        "    \"\"\"\n",
        "    Generate text from the trained model.\n",
        "\n",
        "    Args:\n",
        "        prompt: Starting text (e.g., \"Once upon a time\")\n",
        "        max_length: How many tokens to generate\n",
        "        temperature: Randomness (0.1=deterministic, 1.0=creative)\n",
        "    \"\"\"\n",
        "    model.eval()\n",
        "\n",
        "    # Tokenize prompt\n",
        "    tokens = tokenizer.encode(prompt)\n",
        "    input_ids = torch.tensor(tokens).unsqueeze(0).to(device)\n",
        "\n",
        "    print(f\"🎬 Prompt: '{prompt}'\")\n",
        "    print(f\"📝 Generating {max_length} tokens...\\n\")\n",
        "\n",
        "    generated_tokens = tokens.copy()\n",
        "\n",
        "    with torch.no_grad():\n",
        "        for _ in range(max_length):\n",
        "            # Get model predictions\n",
        "            logits = model(input_ids)\n",
        "\n",
        "            # Get last token's logits\n",
        "            next_token_logits = logits[0, -1, :] / temperature\n",
        "\n",
        "            # Sample from probability distribution\n",
        "            probs = torch.softmax(next_token_logits, dim=-1)\n",
        "            next_token = torch.multinomial(probs, num_samples=1)\n",
        "\n",
        "            # Add to sequence\n",
        "            generated_tokens.append(next_token.item())\n",
        "\n",
        "            # Update input (keep last seq_length tokens)\n",
        "            input_ids = torch.tensor(generated_tokens[-config.max_seq_length:]).unsqueeze(0).to(device)\n",
        "\n",
        "            # Stop if we generate end of text\n",
        "            if next_token.item() == tokenizer.eot_token:\n",
        "                break\n",
        "\n",
        "    # Decode to text\n",
        "    generated_text = tokenizer.decode(generated_tokens)\n",
        "\n",
        "    return generated_text\n",
        "\n",
        "\n",
        "# Test prompts\n",
        "test_prompts = [\n",
        "    \"Once upon a time\",\n",
        "    \"The little girl\",\n",
        "    \"In a magical forest\",\n",
        "]\n",
        "\n",
        "print(\"🧪 Testing your trained model:\\n\")\n",
        "print(\"=\" * 60)\n",
        "\n",
        "for i, prompt in enumerate(test_prompts, 1):\n",
        "    print(f\"\\nTest {i}:\")\n",
        "    print(\"-\" * 60)\n",
        "\n",
        "    generated = generate_text(\n",
        "        model,\n",
        "        tokenizer,\n",
        "        prompt,\n",
        "        max_length=50,\n",
        "        temperature=0.8\n",
        "    )\n",
        "\n",
        "    print(f\"\\n📖 Generated story:\")\n",
        "    print(generated)\n",
        "    print(\"-\" * 60)\n",
        "\n",
        "print(\"\\n✨ Your model can now generate text!\")\n",
        "print()"
      ],
