      "cell_type": "code",
      "source": [
        "print(\"=\" * 60)\n",
        "print(\"STEP 3: CREATE TRAINING PAIRS\")\n",
        "print(\"=\" * 60)\n",
        "\n",
        "class TinyStoriesDataset(Dataset):\n",
        "    \"\"\"\n",
        "    Dataset that creates input→output pairs from tokenized stories.\n",
        "\n",
        "    For each sequence, we predict the next token:\n",
        "    - Input:  [token1, token2, token3]\n",
        "    - Output: [token2, token3, token4]\n",
        "\n",
        "    This is called \"next token prediction\" - the core of language modeling!\n",
        "    \"\"\"\n",
        "\n",
        "    def __init__(self, tokenized_data, seq_length=128):\n",
        "        self.tokenized_data = tokenized_data\n",
        "        self.seq_length = seq_length\n",
        "\n",
        "        # Concatenate all tokens into one long sequence\n",
        "        print(\"🔗 Concatenating all tokens...\")\n",
        "        self. all_tokens = []\n",
        "        for item in tqdm(tokenized_data, desc=\"Processing\"):\n",
        "            self.all_tokens.extend(item['tokens'])\n",
        "\n",
        "        print(f\"✅ Total tokens: {len(self.all_tokens):,}\\n\")\n",
        "\n",
        "    def __len__(self):\n",
        "        # Number of sequences we can create\n",
        "        return (len(self.all_tokens) - 1) // self.seq_length\n",
        "\n",
        "    def __getitem__(self, idx):\n",
        "        # Get a chunk of tokens\n",
        "        start_idx = idx * self.seq_length\n",
        "        end_idx = start_idx + self.seq_length + 1\n",
        "\n",
        "        chunk = self.all_tokens[start_idx:end_idx]\n",
        "\n",
        "        # Input: all tokens except last\n",
        "        # Output: all tokens except first (shifted by 1)\n",
        "        x = torch.tensor(chunk[:-1], dtype=torch.long)\n",
        "        y = torch.tensor(chunk[1:], dtype=torch.long)\n",
        "\n",
        "        return x, y\n",
        "\n",
        "\n",
        "# Create dataset\n",
        "SEQ_LENGTH = 128  # Context window (how many tokens the model sees at once)\n",
        "\n",
        "train_dataset = TinyStoriesDataset(tokenized_dataset, seq_length=SEQ_LENGTH)\n",
        "\n",
        "print(f\"📊 Dataset stats:\")\n",
        "print(f\"   Sequences: {len(train_dataset):,}\")\n",
        "print(f\"   Sequence length: {SEQ_LENGTH} tokens\")\n",
        "print(f\"   Total training tokens: {len(train_dataset) * SEQ_LENGTH:,}\\n\")\n",
        "\n",
        "# Create dataloader\n",
        "BATCH_SIZE = 16  # Adjust based on your GPU memory\n",
        "\n",
        "train_loader = DataLoader(\n",
        "    train_dataset,\n",
        "    batch_size=BATCH_SIZE,\n",
        "    shuffle=True,\n",
        "    num_workers=4  # Set to 2-4 on Mac M1 for faster loading\n",
        ")\n",
        "\n",
        "print(f\"✅ DataLoader created:\")\n",
        "print(f\"   Batch size: {BATCH_SIZE}\")\n",
        "print(f\"   Total batches: {len(train_loader):,}\")\n",
        "print()\n",
        "\n",
        "# Demo: See a training pair\n",
        "x_sample, y_sample = train_dataset[0]\n",
        "print(\"💡 Training Pair Example:\")\n",
        "print(f\"   Input tokens:  {x_sample[:10].tolist()}...\")\n",
        "print(f\"   Output tokens: {y_sample[:10].tolist()}...\")\n",
        "print(f\"\\n   The model learns to predict output given input!\")\n",
        "print()"
      ],
